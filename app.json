{
    "name": "pukiwiki-idobata",
    "description": "Pukiwiki rss items notify idobata.io",
    "repository": "https://github.com/FromAtom/pukiwiki-idobata.git",
    "env": {
        "IDOBATA_HOOK_URL": "<idobata-hook-url>",
        "PUKIWIKI_RSS_URL": "<pukiwiki-rss-url>"
    },
    "addons": [
        "redistogo:nano",
        "scheduler:standard"
    ]
}
